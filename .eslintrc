{
	"root": true,
	"extends": [
		"@nuxtjs/eslint-config-typescript"
	],
	"rules": {
		"vue/multi-word-component-names": "off",
		"vue/no-multiple-template-root": "off",
		"space-before-function-paren": "off",
		"vue/html-indent": "off",
		"no-redeclare": "off",
		"import/named": "off",
		"no-tabs": "off",
		"indent": "off",
		"comma-dangle": "off",
		"semi": "off"
	}
}
